{
  "name": "inhpc_dm",
  "version": "0.7.0",
  "description": "Data Management for InHPC-DE",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/BerndSchuller/inhpc_dm",
  "bugs": {
    "url": "https://github.com/BerndSchuller/inhpc_dm/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jochen Buchholz",
    "email": "buchholz@hlrs.de"
  },
  "contributors": [
    {
      "name": "Bernd Schuller",
      "email": "b.schuller@fz-juelich.de"
    },
    {
      "name": "Myriam Czekala"
    }
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,.js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/BerndSchuller/inhpc_dm.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf inhpc_dm/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.2.0",
    "@jupyterlab/apputils": "^4.2.0",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/docmanager": "^4.2.0",
    "@jupyterlab/docregistry": "^4.0.0",
    "@jupyterlab/filebrowser": "^4.2.0",
    "@jupyterlab/launcher": "^4.2.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/settingregistry": "^4.2.0",
    "@jupyterlab/statusbar": "^4.0.0",
    "@jupyterlab/translation": "^4.0.0",
    "@jupyterlab/ui-components": "^4.2.0",
    "@lumino/algorithm": "^2.0.0",
    "@lumino/commands": "^2.0.0",
    "@lumino/datagrid": "^2.3.1",
    "@lumino/disposable": "^2.1.1",
    "@lumino/messaging": "^2.0.1",
    "@lumino/signaling": "^2.0.0",
    "@lumino/widgets": "^2.3.2",
    "@material-ui/core": "^4.12.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tree-finder": "^0.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.20.2",
    "@jupyterlab/builder": "^4.2.0",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.4.1",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^5.0.1",
    "typescript": "^4.5.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "jupyterlab": {
    "extension": true,
    "outputDir": "inhpc_dm/labextension",
    "schemaDir": "schema"
  }
}
